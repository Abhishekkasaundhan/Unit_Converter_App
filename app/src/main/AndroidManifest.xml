<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unitconverter">

    <application
        android:allowBackup="true"
        android:icon="@drawable/img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/img"
        android:supportsRtl="true"
        android:theme="@style/Theme.Uniconverter">

        <activity android:name="com.example.unitconverter.Splash_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.unitconverter.speed_calculation"></activity>
        <activity android:name="com.example.unitconverter.weight_calculation" />
        <activity android:name="com.example.unitconverter.temp_calculation" />
        <activity android:name="com.example.unitconverter.area_calculation" />
        <activity android:name="com.example.unitconverter.MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>